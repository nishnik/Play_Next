{
  "name": "Play Next : YouTube",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add video to queue",
  "homepage_url": "http://nishantnikhil.github.io",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "omnibox": { "keyword" : "playlist" },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["src/inject/inject.css"],
      "js": ["src/inject/inject.js"]
    }
 ],
  "content_scripts": [
  {
    "matches": [
      "https://*/*"
    ],
    "js": ["Content.js"]
  }
]
  "web_accessible_resources": [
        "src/inject/main.js"
    ]
}
